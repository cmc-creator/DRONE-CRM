# Supabase PostgreSQL connection (Transaction pooler - port 6543 - used by the app)
DATABASE_URL="postgresql://postgres.xxxx:YOUR-PASSWORD@aws-0-us-east-1.pooler.supabase.com:6543/postgres?pgbouncer=true"

# Supabase direct connection (port 5432 - used for db:push and db:seed only)
DIRECT_URL="postgresql://postgres.xxxx:YOUR-PASSWORD@aws-0-us-east-1.pooler.supabase.com:5432/postgres"

# NextAuth
AUTH_SECRET="your-secret-here-run-openssl-rand-base64-32"
AUTH_URL="http://localhost:3000"

# App
NEXTAUTH_URL="http://localhost:3000"

